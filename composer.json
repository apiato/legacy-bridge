{
    "name": "apiato/legacy-bridge",
    "type": "library",
    "description": "A set of utilities to help you gradually upgrade your Apiato codebase.",
    "keywords": [
        "Apiato",
        "PHP"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/apiato/legacy-bridge/issues",
        "source": "https://github.com/apiato/legacy-bridge"
    },
    "authors": [
        {
            "name": "Mohammad Alavi",
            "email": "mohammad.alavi1990@gmail.com",
            "homepage": "https://mohammadalavi.com/",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Apiato\\LegacyBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-protocols": [
            "https"
        ],
        "allow-plugins": {}
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
