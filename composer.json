{
    "name": "mohammad-alavi/apiato-rector",
    "type": "library",
    "description": "A collection of Rectors for the Apiato meta-framework.",
    "keywords": [
        "Apiato",
        "Laravel",
        "Rector",
        "PHP"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mohammad-alavi/apiato-rector/issues",
        "source": "https://github.com/mohammad-alavi/apiato-rector"
    },
    "authors": [
        {
            "name": "Mohammad Alavi",
            "email": "mohammad.alavi1990@gmail.com",
            "homepage": "https://mohammadalavi.com/",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^8.2",
        "webmozart/assert": "^1.11",
        "illuminate/routing": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.3",
        "fakerphp/faker": "^1.19.1",
        "friendsofphp/php-cs-fixer": "^3.8.0",
        "larastan/larastan": "^3.0",
        "mockery/mockery": "^1.4.4",
        "php-standard-library/psalm-plugin": "^2.3",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.0.1",
        "psalm/plugin-laravel": "^3.0",
        "psalm/plugin-mockery": "^1.2",
        "psalm/plugin-phpunit": "^0.19.2",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Apiato\\LegacyBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-protocols": [
            "https"
        ],
        "allow-plugins": {}
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
